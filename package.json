{
  "name": "@emillc/eden",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "author": {
    "name": "ChillCicada",
    "url": "https://chillcicada.com",
    "email": "2210227279@qq.com"
  },
  "license": "MIT",
  "scripts": {
    "dev": "astro dev --port=5555",
    "build": "astro check && astro build",
    "preview": "astro preview --port=4444",
    "test": "vitest run",
    "sync": "astro sync",
    "check": "astro check",
    "astro": "astro",
    "astro:up": "pnpm dlx @astrojs/upgrade",
    "lint": "eslint .",
    "cz": "cz",
    "lint:fix": "eslint --fix .",
    "font:min": "node ./scripts/fontmin",
    "font:prune": "rimraf ./dist/fonts",
    "jampack": "jampack ./dist",
    "postinstall": "simple-git-hooks",
    "postbuild": "node ./scripts/fontmin && jampack ./dist && node ./scripts/postbuild"
  },
  "dependencies": {
    "@iconify-json/mingcute": "1.2.3",
    "@iconify-json/ri": "1.2.5",
    "astro-icon": "1.1.4",
    "fuse.js": "7.0.0",
    "remark-collapse": "0.1.2",
    "remark-emoji": "^5.0.1",
    "remark-toc": "9.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "3.12.1",
    "@astrojs/check": "0.9.4",
    "@astrojs/rss": "4.0.10",
    "@astrojs/sitemap": "3.2.1",
    "@astrojs/svelte": "6.0.2",
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@divriots/jampack": "0.32.0",
    "@types/html-minifier": "4.0.5",
    "@types/node": "22.10.2",
    "@unocss/eslint-plugin": "0.65.3",
    "@unocss/reset": "^0.65.3",
    "astro": "4.16.18",
    "commitizen": "4.3.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "9.17.0",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-format": "0.1.3",
    "eslint-plugin-svelte": "2.46.1",
    "fontmin": "2.0.0",
    "github-slugger": "2.0.0",
    "html-minifier": "4.0.0",
    "lightningcss": "1.28.2",
    "lint-staged": "15.2.11",
    "rimraf": "6.0.1",
    "simple-git-hooks": "2.11.1",
    "terser": "5.37.0",
    "typescript": "5.7.2",
    "typstyle": "0.12.10",
    "unocss": "0.65.3",
    "vitest": "2.1.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "npx commitlint -e $1"
  },
  "lint-staged": {
    "*.{[jt]s,json,y{,a}ml,astro,css,svelte,md}": [
      "eslint --fix"
    ],
    "*.typ": [
      "typstyle -i"
    ]
  },
  "jampack": {}
}
