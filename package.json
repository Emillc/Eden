{
  "name": "@emillc/eden",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "author": {
    "name": "ChillCicada",
    "url": "https://chillcicada.com",
    "email": "2210227279@qq.com"
  },
  "license": "MIT",
  "scripts": {
    "dev": "astro dev --port=5555",
    "build": "astro check && astro build",
    "preview": "astro preview --port=4444",
    "test": "vitest run",
    "sync": "astro sync",
    "check": "astro check",
    "astro": "astro",
    "astro:up": "pnpm dlx @astrojs/upgrade",
    "lint": "eslint .",
    "cz": "cz",
    "lint:fix": "eslint --fix .",
    "font:min": "node ./scripts/fontmin",
    "font:prune": "rimraf ./dist/fonts",
    "jampack": "jampack ./dist",
    "postinstall": "simple-git-hooks",
    "postbuild": "node ./scripts/fontmin && jampack ./dist && node ./scripts/postbuild"
  },
  "dependencies": {
    "@iconify-json/mingcute": "1.2.5",
    "@iconify-json/ri": "1.2.5",
    "astro-icon": "1.1.5",
    "fuse.js": "7.1.0",
    "remark-emoji": "5.0.2",
    "remark-github-blockquote-alert": "1.3.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "3.16.0",
    "@astrojs/check": "0.9.4",
    "@astrojs/rss": "4.0.12",
    "@astrojs/sitemap": "3.5.1",
    "@astrojs/svelte": "7.1.1",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@divriots/jampack": "0.34.0",
    "@types/html-minifier": "4.0.5",
    "@types/node": "22.18.1",
    "@unocss/eslint-plugin": "65.5.0",
    "@unocss/reset": "^65.5.0",
    "astro": "5.13.7",
    "commitizen": "4.3.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "9.35.0",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-format": "1.0.1",
    "eslint-plugin-svelte": "2.46.1",
    "fontmin": "2.0.3",
    "github-slugger": "2.0.0",
    "html-minifier": "4.0.0",
    "lightningcss": "1.30.1",
    "lint-staged": "16.1.6",
    "rimraf": "6.0.1",
    "simple-git-hooks": "2.13.1",
    "svelte": "5.38.8",
    "terser": "5.44.0",
    "typescript": "5.9.2",
    "unocss": "65.5.0",
    "vite": "6.3.6",
    "vitest": "2.1.9"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "npx commitlint -e $1"
  },
  "lint-staged": {
    "*.{[jt]s,json,y{,a}ml,astro,css,svelte,md}": [
      "eslint --fix"
    ]
  }
}
